    "delegate": "^3.1.2",
    "drag-mock": "^1.4.0",
    "element-closest": "^2.0.2",
    "es6-string-html-template": "^1.0.2",
    "es6-symbol": "^3.1.1",
    "expect": "^24.8.0",
    "form-serialize": "^0.7.1",
    "fstream": "^1.0.12",
    "glob": "^7.1.1",
    "htmltidy2": "^0.3.0",
