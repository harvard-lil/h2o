    "serialize-javascript": "2.1.1",
    "set-value": "^2.0.1",
    "v-click-outside": "^3.0.1",
    "vue": "^2.6.10",
    "vue-contenteditable-directive": "^1.2.0",
    "vue-loader": "^15.6.2",
    "vue-nestable": "^2.5.0",
    "vue-router": "^3.0.6",
    "vue-scrollto": "^2.13.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-unit-mocha": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "cache-loader": "^4.1.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "null-loader": "^3.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "sinon": "^7.3.2",
    "tinymce": "^5.1.4",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-tracker": "^0.4.3",
    "webpack-dev-server": "^3.3.1",
    "xhr-mock": "^2.3.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-cond-assign": "off",
      "no-unused-vars": [
        "error",
        {
          "args": "none",
          "ignoreRestSiblings": true,
          "varsIgnorePattern": "^_"
        }
      ],
      "vue/valid-template-root": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "FRONTEND_URLS": true,
      "$": true,
      "jQuery": true,
      "app": true,
      "CKEDITOR": true
    },
    "overrides": [
      {
        "files": [
          "**/*.test.js"
        ],
        "env": {
          "mocha": true
        },
        "globals": {
          "expect": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "edge 16"
  ]
}
