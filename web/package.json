{
  "name": "h2o",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit --require frontend/test/mocha_setup.js frontend/test/**/*.test.js",
    "lint": "vue-cli-service lint frontend/"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
