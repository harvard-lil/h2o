<p>
  <label>Last Updated:</label>
  <%= @case.updated_at.strftime("%B %d, %Y") %>
</p>
<p>
  <label>Decision date:</label>
  <%= @case.decision_date %>
</p>

<% unless @case.author.blank? %>
<p>
  <label>Author:</label>
  <%= @case.author %>
</p>
<% end -%>

<% unless @case.case_citations.blank? %>
<p>
  <label>Citations:</label>
  <%= @case.case_citations.collect{|cc| cc.display_name}.join(', ') %>
</p> 
<% end %>

<% unless @case.case_docket_numbers.blank? %>
<p>
  <label>Docket Numbers:</label>
  <%= @case.case_docket_numbers.collect{|cc| cc.display_name}.join(', ') %>
</p> 
<% end %>

<% unless @case.case_jurisdiction.blank? %>
<p>
  <label>Jurisdiction:</label>
  <%= @case.case_jurisdiction.display_name %>
</p> 
<% end %>
