@font-face {
  font-family: 'h2o-futura-pt';
  src: url('/Futura-Pt-Heavy.otf');
  font-weight: normal;
  font-style: normal;
}

#base_index {
  #shell .main_wrapper,
  #shell .wrapper {
    width: 100%;
    max-width: 100%;
  }
  #shell header .wrapper,
  #shell footer .wrapper {
    max-width: 1094px;
  }
  .inner_wrapper {
    max-width: 1094px;
    margin: 0px auto;
  }

  #home_index {
    * {
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    }
    .welcome {
      border-top: 2px solid #333;
      padding-bottom: 2em;
      a {
        text-decoration: underline;
        color: #036290;
      }
      a:hover{
        color: #333;
        text-decoration: underline;
      }
    }

    h1, p {
      font-family: 'futura-pt', sans-serif;
    }

    #welcome1 {
      background: #036290;
      text-align: center;
      position: relative;
      width: 100%;
      border-top: none;
      h1 {
        font-size: 51px;
        text-align: center;
        padding-top: 44px;
        text-align: center;
        color: #f7f7f7;
        font-weight: bold;
      }
      p {
        color: #FFFFFF;
        font-size: 38px;
        color: #f7f7f7;
        text-align: center;
        color: #F7f7f7;
        text-decoration: none;
        line-height: 38px;
        padding: 2px 5% 10px 5%;
      }
      a {
        color: #FFF;
        text-decoration: underline;
        line-height: 20px;
      }
      a:hover{
        color: #333;
        text-decoration: underline;
      }
      .browsebtn {
        background: #e3e3e3;
        color: #333;
      }
    }
    #welcome2 {
      background: #e3e3e3;
      color: #f7f7f7;
      text-align: center;
      padding-bottom: 5em;
      .browsebtn{
        border: none;
      }
      .browsebtn:hover{
        background: #036290;
        box-shadow: none;
        border: none;
        border: 1px solid #f7f7f7;
      }
      h1 {
        color: #333;
        text-align: center;
        font-size: 36pt;
        padding-top: 25px;
        padding-bottom: 15px;
      }
      p {
        color: #333;
        text-align: center;
        text-decoration: underline;
        font-size: 20pt;
        text-decoration: none;
        padding: 0px 11%;
        line-height: 38.1px;
      }
      a {
        color: #f7f7f7;
        text-align: center;
        text-decoration: underline;
        font-size: 20pt;
        text-decoration: none;
        padding-top: .5em;
        font-weight: 800;
        display: block;
      }
      a:hover{
        color: #036290
      }
      .col-sm-6 {
        width: 44%;
        float: left;
        min-height: 1px;
        position: relative;
        padding: 25px 14px;
      }
      .col-collages {
        padding-left: 64px;
        width: 519px;
        padding-right: 0px;
      }
      .colimg img {
        border: 3px solid #333;
        box-shadow: 0px 2px 5px #333;
        background: #f7f7f7;
      }
    }
    #welcome3 {
      background: #036290;
      text-align: center;
      h1 {
        color: #f7f7f7;
        text-align: center;
        padding-top: 25px;
      }
      p {
        color: #f7f7f7;
        text-align: center;
        text-decoration: underline;
        font-size: 20pt;
        text-decoration: none;
        padding: 25px 11% 10px 11%;
        line-height: 38px;
      }
      a:hover{
        color: #333
      }
      .col-xs-4 {
        float: left;
        min-height: 1px;
        position: relative;
        width: 29.3333%;
        padding-top: 1px;
        padding: 1px 2% 0px 2%;
      }
      .colimg img {
        background: none;
        border: none;
        padding: 1.5em;
        box-shadow: none;
      }
      a {
        color: #f7f7f7;
        text-align: center;
        text-decoration: underline;
        padding-top: .5em;
      }
    }
    #welcome4 {
      background: #e3e3e3;
      text-align: center;
      .browsebtn{
        border: none;
      }
      h1{
        color: #333;
        text-align: center;
        padding-top: 25px;
      }
      p {
        color: #333;
        text-align: center;
        text-decoration: underline;
        font-size: 20pt;
        text-decoration: none;
        padding: 1px 11% 10px 11%;
        line-height: 38px;
        a {
          line-height: 1.0em;
        }
      }
    }
   
    .btn:hover, .btn:hover {
      background: #333;
      color: #f7f7f7;
      opacity: .8;
    }
    .paneltop h1 {
      font-size: 36pt;
      font-weight: 800; 
      text-align: center;
      padding: .3em
    }
    .browsebtn {
      background: #036290;
      color: #FFF;
      font-size: 12pt;
      text-align: center;
      text-transform: uppercase;
      margin-top: 1em;
      font-weight: bold;
      margin-bottom: 1em;
      box-shadow: 0px 2px 5px #333;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    }
    .img-responsive {
      margin:0 auto;
      display: block;
      height: auto;
      max-width: 100%;
      vertical-align: middle;
    }
    .coltxt {
      p {
        padding: 0em 2em 0em 2em;
      }
      img {
        border: 3px solid #333;
        box-shadow: 0px 2px 5px #333;
        background: #f7f7f7;
      }
    }
    .btn-secondary {
      border: 1px solid #ccc;
      -moz-user-select: none;
      cursor: pointer;
      display: inline-block;
      line-height: 1.42857;
      padding: 6px 12px;
      vertical-align: middle;
      white-space: nowrap;
    }
  }
}
