          $('#user_keywords').val(keyword_value);
        }
        var sort_value = $.address.parameter('sort');
        var sort = $('.sort');
        h2o_global.resetSort(sort_value, sort);
        h2o_global.listResults(event.value, false, true);
      }
    }
  });

